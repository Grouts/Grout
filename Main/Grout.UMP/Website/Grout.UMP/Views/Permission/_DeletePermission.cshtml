<div id="permission_delete_confirmation" class="no-padding">
    <div class="col-lg-12 no-padding" id="PopupContainer">
        <div class="col-md-12 header-menu">
            <div class="col-xs-6 no-padding">
                <span class="su su-delete Head-icon"></span>
                <span class="PopupTitle">Delete Permission</span>
            </div>
            <div class="col-xs-6 no-padding">
                <a href="javascript:void(0);" onclick="onPermissionDialogClose()" class="PopupClose"><span class="su su-close"></span></a>
                <div id="folder-setting-arrow" class="downArrow" style="left: 37px;"></div>
            </div>
        </div>
        <div class="dialogBody col-xs-12 no-padding">
            <div class="col-xs-12 no-left-padding padding20 no-bottom-padding no-right-padding">
                <div class="deleteMsg">Are you sure you want to delete this permission?</div>
            </div>
            <div class="col-xs-12 dialogFooterMar rightAlign no-right-padding no-left-padding">
                <div class="col-xs-4"><input type="hidden" id="hiddenId" value="@ViewBag.id" /></div>
                <div class="col-xs-8 no-padding rightAlign">
                    <input type="hidden" value="" id="delete_user_id">
                    <input type="button" class="btn btn-danger rightAlign" id="delete_permission" value="Yes" />
                    <input type="button" onclick="onPermissionDialogClose()" class="btn btn-link rightAlign PopupClose" value="No" />
                </div>
            </div>
        </div>
    </div>
</div>